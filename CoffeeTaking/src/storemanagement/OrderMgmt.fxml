<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="storemanagement.OrderMgmtController">
   <children>
      <Label fx:id="Title_OrderMgmt" layoutX="38.0" layoutY="40.0" text="주문 관리">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="BTN_CurrentOrder" layoutX="79.0" layoutY="144.0" mnemonicParsing="false" onAction="#CurrentOrderClicked">
         <graphic>
            <ImageView fx:id="Img_CurrentOrder" fitHeight="165.0" fitWidth="213.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/CurrentOrder.JPG" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="BTN_PastOrder" layoutX="340.0" layoutY="144.0" mnemonicParsing="false">
         <graphic>
            <ImageView fx:id="Img_PastOrder" fitHeight="165.0" fitWidth="176.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/PastOrders.JPG" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
