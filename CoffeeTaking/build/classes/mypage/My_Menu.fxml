<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mypage.My_MenuController">
   <children>
      <TableView fx:id="TB_MyMenuList" layoutX="44.0" layoutY="97.0" prefHeight="245.0" prefWidth="495.0">
        <columns>
            <TableColumn fx:id="TB_CafeName" prefWidth="146.0" text="카페" />
          <TableColumn fx:id="TB_MenuName" prefWidth="166.0" text="메뉴" />
          <TableColumn fx:id="TB_MaterialQuantity" minWidth="0.0" prefWidth="182.0" text="재료의 양" />
        </columns>
      </TableView>
      <Button fx:id="BTN_Add" layoutX="398.0" layoutY="353.0" mnemonicParsing="false" text="추가" />
      <Button fx:id="BTN_Change" layoutX="448.0" layoutY="353.0" mnemonicParsing="false" text="수정" />
      <Button fx:id="BTN_Delete" layoutX="499.0" layoutY="353.0" mnemonicParsing="false" onAction="#btnDeleteClick" text="삭제" />
      <TextField alignment="CENTER" layoutX="15.0" layoutY="19.0" prefHeight="52.0" prefWidth="248.0" style="-fx-border-color: #000000;" text="나만의 메뉴 목록">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </TextField>
      <Button fx:id="BTN_BackToMenu_Consumer" layoutX="292.0" layoutY="45.0" mnemonicParsing="false" onAction="#Push_BTN_BackToMenu_Consumer" text="메인 메뉴로">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
