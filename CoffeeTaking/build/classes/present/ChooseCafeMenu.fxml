<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="present.ChooseCafeMenuController">
   <children>
      <Button fx:id="btn_next_CCM" layoutX="277.0" layoutY="332.0" mnemonicParsing="false" onAction="#btnSelect2" text="Next" />
      <TabPane layoutX="68.0" layoutY="88.0" prefHeight="236.0" prefWidth="447.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="TP_Cafe" text="Select Cafe">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="TB_Cafe" layoutX="7.0" layoutY="2.0" prefHeight="200.0" prefWidth="200.0">
                          <columns>
                            <TableColumn fx:id="Name_TB_Cafe" prefWidth="75.0" text="Cafe Name" />
