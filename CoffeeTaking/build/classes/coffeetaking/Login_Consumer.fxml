<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coffeetaking.Login_ConsumerController">
   <children>
      <Label fx:id="Label_ID" layoutX="160.0" layoutY="138.0" prefHeight="23.0" prefWidth="58.0" text="ID" />
      <Label fx:id="Label_Password" layoutX="160.0" layoutY="199.0" prefHeight="23.0" prefWidth="58.0" text="Password" />
      <TextField fx:id="TextField_ID" layoutX="218.0" layoutY="138.0" prefHeight="23.0" prefWidth="204.0" />
      <PasswordField fx:id="TextField_Password" layoutX="218.0" layoutY="199.0" prefHeight="23.0" prefWidth="204.0" />
      <Label fx:id="Label_Main_Title" layoutX="30.0" layoutY="25.0" prefHeight="90.0" prefWidth="290.0" text="커피들고 가는 길" textFill="#d9820b">
         <font>
            <Font name="HYHeadLine-Medium" size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="ImageView_Main_Icon" fitHeight="120.0" fitWidth="60.0" layoutX="310.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/CoffeeTaking%20Icon.png" />
         </image>
      </ImageView>
      <Button fx:id="BTN_Login" layoutX="422.0" layoutY="290.0" mnemonicParsing="false" onAction="#Push_BTN_Login" prefHeight="48.0" prefWidth="74.0" text="로그인">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
