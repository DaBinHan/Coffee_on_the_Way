<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coffeetaking.Menu_ConsumerController">
   <children>
      <Label fx:id="Label_Main_Title" layoutX="40.0" layoutY="35.0" prefHeight="90.0" prefWidth="290.0" text="커피들고 가는 길" textFill="#d9820b">
         <font>
            <Font name="HYHeadLine-Medium" size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="ImageView_Main_Icon" fitHeight="120.0" fitWidth="60.0" layoutX="320.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/CoffeeTaking%20Icon.png" />
         </image>
      </ImageView>
      <Button fx:id="BTN_Order" layoutX="40.0" layoutY="200.0" mnemonicParsing="false" onAction="#Push_BTN_Order" prefHeight="48.0" prefWidth="79.0" text="주문하기">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
      <Button fx:id="BTN_Review" layoutX="170.0" layoutY="200.0" mnemonicParsing="false" onAction="#Push_BTN_Review" prefHeight="48.0" prefWidth="87.0" text="리뷰 작성">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
      <Button fx:id="BTN_MyPage" layoutX="300.0" layoutY="200.0" mnemonicParsing="false" onAction="#Push_BTN_MyPage" prefHeight="48.0" prefWidth="94.0" text="마이페이지">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
      <Button fx:id="BTN_Present" layoutX="430.0" layoutY="200.0" mnemonicParsing="false" onAction="#Push_BTN_Present" prefHeight="48.0" prefWidth="79.0" text="선물하기">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Button>
      <Label fx:id="Label_ConsumerName" layoutX="397.0" layoutY="35.0" prefHeight="68.0" prefWidth="200.0">
         <font>
            <Font name="HYHeadLine-Medium" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
